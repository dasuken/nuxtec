{
  "name": "my-ec",
  "version": "1.0.0",
  "description": "My majestic Nuxt.js project",
  "author": "github daisukenoguchi",
  "private": true,
  "config": {
    "s3BucketName": "tk.nuxtec.assets.nuxtec.tk",
    "region": "ap-northeast-1",
    "_nuxtDir": ".nuxt/dist/client",
    "assetsDir": "client/static/assets",
    "cloudFormationStackName": "NuxtExpressSame",
    "functionName": "AwsServerlessExpressFunction",
    "accountId": "231421272561"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon server/index.js --watch server",
    "start": "NODE_ENV=production node server/index.js",
    "build": "rm -rf .nuxt/dist && nuxt build",
    "nuxt-start": "nuxt start",
    "generate": "nuxt generate",
    "heroku-postbuild": "npm run build",
    "cf-clear": "aws cloudfront create-invalidation --distribution-id E2QTJHMA5Q0N00 --paths '/*'",
    "s3-ls": "aws s3 ls $npm_package_config_s3BucketName",
    "deploy-dist": "aws s3 sync .nuxt/dist/client s3://$npm_package_config_s3BucketName/_nuxt --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "deploy-assets": "aws s3 sync client/static/assets s3://$npm_package_config_s3BucketName/assets --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "delete-dist": "aws s3 rm s3://$npm_package_config_s3BucketName/_nuxt --recursive",
    "delete-assets": "aws s3 rm s3://$npm_package_config_s3BucketName/assets --recursive",
    "deploy-s3": "npm run delete-dist && npm run deploy-dist",
    "deploy-s3-gw": "aws s3 sync .nuxt/dist/client s3://$npm_package_config_s3BucketName/nuxt --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "deploy": "npm run build && npm run deploy-s3 && sls deploy"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/now-builder": "^0.17.2",
    "@nuxtjs/toast": "^3.3.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@stripe/stripe-js": "^1.7.0",
    "aws-sdk": "^2.705.0",
    "aws-serverless-express": "^3.3.8",
    "bcryptjs": "^2.4.3",
    "consola": "^2.13.0",
    "cookie-parser": "^1.4.5",
    "cookie-universal-nuxt": "^2.1.4",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dayjs": "^1.8.28",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-cache-controller": "^1.1.0",
    "graceful-fs": "^4.2.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "mongoose": "^5.9.18",
    "mongoose-deep-populate": "^3.1.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemon": "^2.0.4",
    "nuxt": "^2.0.0",
    "s3": "^4.4.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^4.1.1",
    "serverless-http": "^2.5.0",
    "stripe": "^8.56.0",
    "vuex-persistedstate": "^3.0.1"
  }
}
